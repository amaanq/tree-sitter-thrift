{
  "name": "tree-sitter-thrift",
  "version": "0.3.0",
  "description": "Thrift grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer",
    "thrift"
  ],
  "author": "duskmoon (Campbell He)",
  "contributors": [
    "Amaan Qureshi <amaanq12@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duskmoon314/tree-sitter-thrift/issues"
  },
  "dependencies": {
    "nan": "^2.16.0"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "0.20.7"
  },
  "repository": "https://github.com/duskmoon314/tree-sitter-thrift",
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test && script/parse-examples",
    "parse": "tree-sitter parse",
    "test-windows": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.thrift",
      "file-types": [
        "thrift"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
